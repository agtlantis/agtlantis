{
    "name": "@agtlantis/core",
    "version": "0.4.0",
    "description": "AI SDK 위에서 Agent와 Pipeline을 쉽고 일관되게 구축하는 프레임워크",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./testing": {
            "types": "./dist/testing/index.d.ts",
            "import": "./dist/testing/index.js",
            "require": "./dist/testing/index.cjs"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "rm -rf dist && tsup",
        "test": "vitest run src/",
        "test:unit": "vitest run src/",
        "test:e2e": "REAL_AI_ENABLED=true vitest run e2e/",
        "test:watch": "vitest",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "handlebars": "^4.7.8",
        "lodash": "^4.17.23",
        "yaml": "^2.8.2",
        "zod-to-json-schema": "^3.25.1"
    },
    "peerDependencies": {
        "@ai-sdk/google": "^3.0.0",
        "@ai-sdk/openai": "^3.0.0",
        "@google/genai": "^1.0.0",
        "ai": "^6.0.0",
        "zod": "^3.23.0 || ^4.0.0"
    },
    "peerDependenciesMeta": {
        "@ai-sdk/google": {
            "optional": true
        },
        "@ai-sdk/openai": {
            "optional": true
        },
        "@google/genai": {
            "optional": true
        }
    },
    "devDependencies": {
        "@ai-sdk/google": "^3.0.10",
        "@ai-sdk/openai": "^3.0.12",
        "@google/genai": "^1.37.0",
        "@types/lodash": "^4.17.23",
        "ai": "^6.0.39",
        "dotenv": "^17.2.3",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "vitest": "^4.0.17",
        "zod": "^4.3.5"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "ai",
        "agent",
        "llm",
        "framework"
    ],
    "license": "MIT"
}
